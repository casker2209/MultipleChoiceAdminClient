<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label layoutX="40.0" layoutY="60.0" prefHeight="40.0" prefWidth="80.0" text="Tên" />
      <Label layoutX="40.0" layoutY="150.0" prefHeight="40.0" prefWidth="80.0" text="Mật khẩu" />
      <TextField layoutX="120.0" layoutY="60.0" prefHeight="40.0" prefWidth="240" fx:id="NameField"/>
      <PasswordField layoutX="120.0" layoutY="150.0" prefHeight="40.0" prefWidth="240.0" fx:id="PassField" />
      <Button layoutX="150.0" layoutY="240.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="90.0" text="Login" onAction="#Login" fx:id="LoginButton"/>
   </children>
</AnchorPane>
